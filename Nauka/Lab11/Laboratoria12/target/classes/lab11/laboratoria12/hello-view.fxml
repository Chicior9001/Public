<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="850.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lab11.laboratoria12.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <GridPane prefHeight="580.0" prefWidth="810.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="695.0" minWidth="10.0" prefWidth="599.0" />
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="354.0" minWidth="0.0" prefWidth="211.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints maxHeight="550.0" minHeight="10.0" prefHeight="404.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="245.0" minHeight="94.0" prefHeight="156.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
          <Button onAction="#onSendButtonClick" prefHeight="57.0" prefWidth="152.0" text="Send!" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
            <font>
               <Font name="Calibri" size="32.0" />
            </font>
         </Button>
         <TextField fx:id="textField" alignment="TOP_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onAction="#onSendButtonClick" GridPane.rowIndex="1">
            <font>
               <Font size="22.0" />
            </font></TextField>
         <TextArea fx:id="outputArea" editable="false" prefHeight="471.0" prefWidth="571.0" wrapText="true">
            <font>
               <Font size="22.0" />
            </font></TextArea>
         <ListView fx:id="userList" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
      </children>
   </GridPane>
</VBox>
